{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {},
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": "error",
    "no-undef": "error",
    "no-unreachable": "error",
    "no-unused-vars": "error",
    "valid-typeof": "warn",
    "no-console": "warn"
  }
}
